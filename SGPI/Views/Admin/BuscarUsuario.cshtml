@model Usuario
@{
    ViewData["Title"] = "BuscarUsuario";
}
<div class="card border-success bg-success mb-3" style="max-width: 1200px;">
    <p><a href="" class="colo">Cerrar Sesión</a></p>
    <div class="row no-gutters">
        <div class="col-md-2">
            <img src="data:image/png;base64,"
                 class="card-img imge" alt="60px">
        </div>
        <div class="col-md-8">
            <div class="card-header bg-success text-white"><h1>Buscar Usuario</h1></div>
            <div class="card-body">
                <form method="post">
                    <div class="input-group mb-3">
                        <input type="text" class="form-control" placeholder="Ingrese Documento" aria-label="Ingrese Documento" aria-describedby="basic-addon2">
                        <div class="input-group-append">
                            <button class="btn btn-primary" type="button">Search</button>
                        </div>
                    </div>
                    <br />
                    <table class="table table-dark table-striped" border="1">
                        <thead>
                            <tr>
                                <th scope="col">@Html.DisplayNameFor(model => model.TipoDoc)</th>
                                <th scope="col">@Html.DisplayNameFor(model => model.NumDoc)</th>
                                <th scope="col">@Html.DisplayNameFor(model => model.Nombre)</th>
                                <th scope="col">@Html.DisplayNameFor(model => model.Apellido)</th>
                                <th scope="col">@Html.DisplayNameFor(model => model.Programa)</th>
                                <th scope="col">@Html.DisplayNameFor(model => model.Rol)</th>
                                <th scope="col">FechaMod</th>
                                <th scope="col">Opciones</th>
                            </tr>
                        </thead>
                        <tbody>
                            <tr>

                                <td>@Html.DisplayFor(model => model.TipoDoc)</td>
                                <td>@Html.DisplayFor(model => model.NumDoc)</td>
                                <td>@Html.DisplayFor(model => model.Nombre)</td>
                                <td>@Html.DisplayFor(model => model.Apellido)</td>
                                <td>@Html.DisplayFor(model => model.Programa)</td>
                                <td>@Html.DisplayFor(model => model.Rol)</td>
                                <td>123</td>
                                <td>
                                    <input type="button" value="Editar">
                                    &nbsp;<input type="button" value="Borrar">
                                </td>
                            </tr>
                        </tbody>
                    </table>
                    <button type="button" class="btn btn-primary">Aceptar</button>
                    <button type="submit" class="btn btn-danger">Cancelar</button>
                </form>
            </div>
        </div>
    </div>
</div>


    
    
