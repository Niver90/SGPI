@model Usuario
@{
    ViewData["Title"] = "Homologar";
}
<div class="card border-success bg-success mb-3" style="max-width: 1200px;">
    <p><a href="" class="colo">Cerrar Sesión</a></p>
    <div class="row no-gutters">
        <div class="col-md-2">
            <img src="data:image/png;base64,"
                 class="card-img imge" alt="60px">
        </div>
        <div class="col-md-8">
            <div class="card-header bg-success text-white"><h1>Homologar</h1></div>
            <div class="card-body">
                <form method="post">
                    <div class="input-group mb-2">
                        <input type="text" class="form-control" placeholder="Ingrese Documento del Estudiante" aria-label="Ingrese Documento" aria-describedby="basic-addon2">
                        <div>
                            <button class="btn btn-primary" type="submit">Search</button>
                        </div>
                    </div>
                    <div class="form-row">
                        <div class="form-group col-md-4">
                            <label for="FechaElab" class="col-form-label">Fecha Elaboracion</label>
                            <input type="text" class="form-control" id="FechaElab">
                        </div>
                        <div class="form-group col-md-4">
                            <label asp-for="Nombre" class="col-form-label">Nombre</label>
                            <input type="text" class="form-control" id="Nombre">
                        </div>
                        <div class="form-group col-md-4">
                            <label asp-for="Documento" class="col-form-label">Documento</label>
                            <input type="text" class="form-control" id="Apellido">
                        </div>
                    </div>
                    <div class="form-row">
                        <div class="form-group col-md-4">
                            <label asp-for="IdPrograma" class="col-form-label">Programa</label>
                            <input type="text" class="form-control" id="Programa">
                        </div>
                        <div class="form-group col-md-4">
                            <label for="PerodoAcademico" class="col-form-label">Periodo Academico</label>
                            <input type="date" class="form-control" id="PeriodoAcademico">
                        </div>
                        <div class="form-group col-md-4">
                            <label for="TipoHomologacion" class="col-form-label">Tipo Homologacion</label>
                            <select id="TipoDoc" class="form-control">
                                <option selected>Choose...</option>
                                <option>Transf.Interna</option>
                                <option>Transf.Externa</option>
                            </select>
                        </div>
                    </div>

                    <br />
                    <table class="table table-dark table-striped" border="1">
                        <thead>
                            <tr>
                                <th scope="col">No.</th>
                                <th scope="col">Cod</th>
                                <th scope="col">Asign</th>
                                <th scope="col">Cred</th>
                                <th scope="col">Nivel</th>
                                <th scope="col">Progr</th>
                                <th scope="col">Univ</th>
                                <th scope="col">Asig.Hom</th>
                                <th scope="col">Cod.A.H</th>
                                <th scope="col">Cred</th>
                                <th scope="col">Nota</th>
                            </tr>
                        </thead>
                        <tbody>
                            <tr>
                                <td></td>
                                <td><input type="text" name="codigo" size="1" /></td>
                                <td><input type="text" name="Asignatura" size="2" /></td>
                                <td><input type="text" name="Creditos" size="1" /></td>
                                <td><input type="text" name="Nivel" size="1" /></td>
                                <td><input type="text" name="Programa" size="2" /></td>
                                <td><input type="text" name="Universidad" size="2" /></td>
                                <td>
                                    <select id="AsigHomo"><option selected>Choose...</option></select>
                                </td>
                                <td><input type="text" name="CodAsigHomo" size="1" /></td>
                                <td><input type="text" name="Creditos" size="1" /></td>
                                <td><input type="text" name="Nota" size="1" /></td>
                            </tr>
                        </tbody>
                    </table>
                    <button type="submit" class="btn btn-primary">Guardar</button>
                </form>
            </div>
        </div>
    </div>
</div>